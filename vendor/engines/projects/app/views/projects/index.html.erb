<% content_for :body_content_left do %>
  <ul id="projects">
    <% @projects.group_by(&:semester).sort.reverse.each do |semester, projects| %>
      <li>
        <h3><%= semester %></h3>
        <ul>
          <% projects.each do |project| %>
            <li>
				<div class="project">
					<!--<a class="project-title" id="<%= "project-title-#{project.id}" %>"><%= project.title %></a>-->
					<%= link_to "Editar", edit_admin_project_path(project), :title=>"", :class => "edit" if user_signed_in? %>
					<h4><%= project.title %></h4>
					<!--<img alt="" src="/images/trabalho/trabalho1.png">-->
					<div class="project-description"><%=raw project.description %></div>
					<!--<div id="<%= "project-#{project.id}-content" %>" class="project-content"></div>-->
              </div>
            </li>
          <% end %>
        </ul>
      </li>

    <% end %>
    <%= raw "<div>" %>
  </ul>
  <% if @project %>
    <div class="unfold" style="display: none;" id="<%=  @project.id %>"/>
  <% end %>

<% end %>

<%= render :partial => "/shared/content_page" %>